	version: 0;
	min_compatible_version: 0;
}

/*TEMPLATES, credit to Polish Building Set and Quast65 templates*/

template tmpl_sprite_ground(x, y, z) {
// [left_x,   upper_y,    width,    height,    offset_x,    offset_y]
    [x,         y,        68*z,      35*z,      -33*z,        -2*z]
}
template tmpl_sprite_build40(x, y, z) {
// [left_x,   upper_y,    width,    height,    offset_x,    offset_y]
    [x,         y,        68*z,      75*z,      -33*z,       -42*z]
}
template tmpl_sprite_build80(x, y, z) {
// [left_x,   upper_y,    width,    height,    offset_x,    offset_y]
    [x,         y,        68*z,     115*z,      -33*z,       -82*z]
}
template tmpl_sprite_build120(x, y, z) {
// [left_x,   upper_y,    width,    height,    offset_x,    offset_y]
    [x,         y,        68*z,     155*z,      -32*z,      -122*z]
}

// 1x2
template template_1x2_t1(x,y,filename) {
// [left_x,   upper_y,    width,    height,    offset_x,    offset_y]
   [x+32,     y,          96,       284,       -31,         -253,     filename]
}

template template_1x2_t2(x,y,filename) {
// [left_x,   upper_y,    width,    height,    offset_x,    offset_y]
   [x+64,     y,          64,       300,       -31,         -269,     filename]
}


// 2x1
//template for sprite tile.1
template template_2x1_t1(x,y,filename) {
    [x+32, y, 64, 284, -31, -253, filename]
}

//template for sprite tile.2
template template_2x1_t2(x,y,filename) {
    [x, y, 64, 300, -31, -269, filename]
}

template template_2x2_purchase(x,y,filename) {
    [x, y, 128, 250, -95, -219, filename]
    [x+133, y, 128, 200, -95, -169, filename]
    [x+266, y, 128, 200, -95, -169, filename]
    [x+399, y, 128, 200, -95, -169, filename]
	
}

